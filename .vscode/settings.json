{
  "prettier.disableLanguages": ["js", "jsx", "ts"],
  "editor.tabSize": 5,
  "editor.formatOnSave": false,
  "[javascript]": {
    "editor.formatOnSave": false
    
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": false,
    "source.fixAll.eslint": false,
    "source.organizeImports": false
  }
}
